mut u8 x := 0;
mut u8 y := 10;
bool condition := true;

if (x < y and condition) {
    print "first block";
    x := x + 1;
} else {
    if (x = y or !condition) {
        print "second block";
    } else {
        print "third block";
    }
}

mut u8 i;
for (i := 0; i < 5; i := i + 1) {
    print i;
}
